module Stroma
  # Note: Entry uses Data.define but RBS should NOT inherit from Data
  # per RBS documentation for proper type checking
  class Entry
    attr_reader key: Symbol
    attr_reader extension: Module
    attr_reader matrix_name: Symbol

    def initialize: (Symbol key, Module extension, Symbol matrix_name) -> void
                  | (key: Symbol, extension: Module, matrix_name: Symbol) -> void

    def phase_method: () -> Symbol
  end
end
